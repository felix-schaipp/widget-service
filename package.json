  },
  "homepage": "https://github.com/felix-schaipp/widget-service#readme",
  "license": "MIT",
  "engines": {
    "node": "16.3.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1"
  }
