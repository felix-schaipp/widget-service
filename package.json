{
  "name": "ios-widget-service",
  "version": "1.0.0",
  "description": "Small express backend service to provide the ios widget with data",
  "type": "module",
  "private": "true",
  "main": "index.js",
  "repository": "git@github.com:felix-schaipp/widget-service.git",
  "author": "Felix Schaipp <felix.schaipp@auteon.de>",
  "scripts": {
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "start": "node index.js",
    "start:dev": "nodemon index.js",
    "prepare": "husky install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
